      <children>
        <VBox VBox.vgrow="ALWAYS">
          <HBox prefHeight="23.0" spacing="10.0" alignment="TOP_CENTER">
            <children>
              <Label prefWidth="50.000099999997474" text="stats" />
              <TextField fx:id="textstats" prefWidth="465.0">
                <effect>
                  <DropShadow />
                </effect>
              </TextField>
              <CheckBox mnemonicParsing="false" selected="true" text="RE" wrapText="false" />
            </children>
          </HBox>
          <HBox prefHeight="23.0" spacing="10.0" alignment="TOP_CENTER">
